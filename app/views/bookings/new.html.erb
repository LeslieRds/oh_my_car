<div class="container">
    <%= simple_form_for [@car, @booking] do |f| %>
    <%= f.input :start_date, as: :date, input_html: { data: { controller: 'datepicker' } } %>
    <%= f.input :end_date, as: :date, input_html: { data: { controller: 'datepicker' } } %>
    <%= f.button :submit %>
    <% end %>
</div>
